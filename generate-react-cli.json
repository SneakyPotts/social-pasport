{
  "usesTypeScript": true,
  "usesCssModule": true,
  "cssPreprocessor": "scss",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "customTemplates": {
        "component": "templates/default/TemplateName.tsx",
        "index": "templates/default/index.ts",
        "style": "templates/default/TemplateName.module.scss",
        "props": "templates/default/TemplateName.props.ts"
      },
      "path": "components",
      "withLazy": false,
      "withStory": false,
      "withStyle": true,
      "withTest": false,
      "withIndex": true,
      "withProps": true
    }
  },
  "usesStyledComponents": false
}
